{
  "name": "FruitHub",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "author": "Chuanzhao Huang & Zhehao Xu",
  "license": "MIT",
  "scripts": {
    "build": "npm install && cd front && npm install",
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "node ./node_modules/nodemon/bin/nodemon.js ./bin/www.js",
    "frontend": "cd front && npm run dev",
    "initDB": "mongoimport -d photoSharing -c photos --jsonArray db/MOCK_DATA.json"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "~1.4.4",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongo": "^0.1.0",
    "mongodb": "^6.2.0",
    "morgan": "~1.9.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.8.1",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=18.18.0 <19.0.0"
  }
}
